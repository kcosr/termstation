{
  "name": "termstation-backend",
  "version": "1.0.0",
  "description": "termstation Backend - Node.js implementation using node-pty",
  "main": "server.js",
  "type": "module",
  "bin": {
    "agents": "./bootstrap/bin/agents.mjs"
  },
  "scripts": {
    "start": "node server.js",
    "build": "node scripts/build-tools.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:clean": "rm -rf tests/tmp-* /tmp/termstation-tests /tmp/ts-*",
    "test:fork": "vitest run tests/fork-behavior.test.mjs"
  },
  "keywords": [
    "terminal",
    "pty",
    "websocket",
    "session-manager"
  ],
  "author": "termstation Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-pty": "^1.0.0",
    "shell-quote": "^1.8.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/ui": "^2.1.0",
    "vitest": "^2.1.0"
  }
}
